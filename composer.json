{
    "name": "cyber-duck/laravel-alternative-mailer",
    "description": "Allow Laravel to send emails through two different mail configurations.",
    "license": "MIT",
    "keywords": ["laravel","mail","multiple mail configurations"],
    "homepage": "http://github.com/Cyber-Duck/Laravel-Alternative-Mailer",
    "authors": [
        {
            "name": "Simone Todaro",
            "email": "simo.todaro@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/mail": "^5.1.0"
    },
     "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "2.*",
        "laravel/laravel": "~5.1",
        "mockery/mockery": "~0.9.4",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "psr-4": {
            "Cyberduck\\Mail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cyberduck\\Tests\\Mail\\": "tests/"
        }
    },
    "suggest": {
        "aws/aws-sdk-php": "Required to use the SES mail driver (~3.0).",
        "guzzlehttp/guzzle": "Required to use the Mailgun and Mandrill mail drivers (~5.3|~6.0).",
        "jeremeamia/superclosure": "Required to be able to serialize closures (~2.0)."
    },
    "minimum-stability": "stable"
}
